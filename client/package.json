  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0-beta.0",
    "browserify": "^16.2.3",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1",
    "lowdb": "^1.0.0",
    "mdi-material-ui": "^5.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-color": "^2.17.2",
    "react-dom": "^16.6.3",
    "react-route": "^1.0.3",
    "react-router-dom": "^5.0.0",
